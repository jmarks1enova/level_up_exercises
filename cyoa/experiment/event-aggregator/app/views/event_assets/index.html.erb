<h1>Listing event_assets</h1>

<table>
  <thead>
    <tr>
      <th>Calendar event</th>
      <th>Caption</th>
      <th>Mime type</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_assets.each do |event_asset| %>
      <tr>
        <td><%= event_asset.calendar_event_id %></td>
        <td><%= event_asset.caption %></td>
        <td><%= event_asset.mime_type %></td>
        <td><%= event_asset.data %></td>
        <td><%= link_to 'Show', event_asset %></td>
        <td><%= link_to 'Edit', edit_event_asset_path(event_asset) %></td>
        <td><%= link_to 'Destroy', event_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event asset', new_event_asset_path %>
